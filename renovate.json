{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:js-app",
    ":maintainLockFilesWeekly",
    ":semanticCommitTypeAll(build)",
    ":label(dependencies)",
    ":automergeMinor",
    ":rebaseStalePrs"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["prisma", "@prisma/client"],
      "groupName": "Prisma"
    },
    {
      "matchPackagePatterns": ["^@vueuse"],
      "groupName": "VueUse"
    }
  ]
}
